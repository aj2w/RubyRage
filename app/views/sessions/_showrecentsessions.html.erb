<% @online_users.each do |user| %>
<table class="list_of_users" align="center">
  <% if user.username != @current_user.username %>
  <tr>
    <td><%= user.username %></td>
    <td><%= link_to 'Battle', games_multi_path(:username => user.username), class: "request-battle", id: "#{user.username}" %> </td>
  </tr>
  <% end %>
</table>
<% end %>